{
	"name": "path-rush",
	"version": "1.4.1",
	"description": "A simple and powerful plugin for Vite with file-based routing",
	"main": "dist/plugin.js",
	"module": "dist/plugin.mjs",
	"types": "dist/plugin.d.ts",
	"type": "module",
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"prepublishOnly": "npm run build",
		"type-check": "tsc --noEmit"
	},
	"keywords": [
		"vite",
		"plugin",
		"file-based-routing",
		"react",
		"router",
		"typescript",
		"seo",
		"hmr",
		"spa",
		"routing",
		"file-system",
		"automatic-routes"
	],
	"author": {
		"name": "da-b1rmuda",
		"url": "https://github.com/da-b1rmuda"
	},
	"license": "MIT",
	"homepage": "https://github.com/da-b1rmuda/path-rush#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/da-b1rmuda/path-rush.git"
	},
	"bugs": {
		"url": "https://github.com/da-b1rmuda/path-rush/issues"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"dependencies": {
		"fast-glob": "^3.3.2"
	},
	"peerDependencies": {
		"react": "^18.0.0 || ^19.0.0",
		"react-dom": "^18.0.0 || ^19.0.0",
		"react-router-dom": "^6.0.0 || ^7.0.0",
		"vite": "^5.0.0 || ^6.0.0 || ^7.0.0"
	},
	"peerDependenciesMeta": {
		"react": {
			"optional": false
		},
		"react-dom": {
			"optional": false
		},
		"react-router-dom": {
			"optional": false
		},
		"vite": {
			"optional": false
		}
	},
	"devDependencies": {
		"@types/node": "^20.10.0",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"tsup": "^8.0.0",
		"typescript": "^5.4.0",
		"vite": "^5.2.0"
	},
	"exports": {
		".": {
			"types": "./dist/plugin.d.ts",
			"import": "./dist/plugin.mjs",
			"require": "./dist/plugin.js"
		},
		"./react": {
			"types": "./dist/react.d.ts",
			"import": "./dist/react.mjs",
			"require": "./dist/react.js"
		},
		"./core": {
			"types": "./dist/core.d.ts",
			"import": "./dist/core.mjs",
			"require": "./dist/core.js"
		},
		"./package.json": "./package.json"
	},
	"publishConfig": {
		"access": "public"
	}
}
